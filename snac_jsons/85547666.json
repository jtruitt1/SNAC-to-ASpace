    "biogHists": [
        {
            "dataType": "BiogHist",
            "language": {
                "dataType": "Language",
                "language": {
                    "id": "130",
                    "term": "eng",
                    "type": "language_code",
                    "description": "English"
                },
                "script": {
                    "id": "586",
                    "term": "Latn",
