    "resourceRelations": [
        {
            "dataType": "ResourceRelation",
            "resource": {
                "dataType": "Resource",
                "documentType": {
                    "id": "696",
                    "term": "ArchivalResource",
                    "uri": "http://socialarchive.iath.virginia.edu/control/term#ArchivalResource",
                    "type": "document_type"
                },
                "link": "http://www.worldcat.org/oclc/122316582",
                "source": "<objectXMLWrap>\n               <mods xmlns=\"http://www.loc.gov/mods/v3\">\n                  <recordInfo>\n                     <recordOrigin>WorldCat:122316582</recordOrigin>\n                     <recordContentSource>ISIL:OCLC-QQR</recordContentSource>\n                  </recordInfo>\n                  <titleInfo>\n                     <title>Foster-Clement Collection, ca. 1676-1875.</title>\n                  </titleInfo>\n                  <abstract>Papers relating to early settlements in and largely to the Clement and Foster families of Burlington and Gloucester Counties, New Jersey: deeds (1677-1875); wills (1681-1867); family papers (1761-1819), which includes papers and receipts of Josiah Foster and William Foster, draft of William Foster's land, will of William Foster, will of Josiah Foster, Clement and Foster genealogies, Long Book (account book) of Benjamin Robinson, (1784) and surveying notebooks of Josiah Foster (1778-79, 1794-95), Account of Sleepcreek Mill book, of which Josiah Foster was director, (1785-86), and correspondence largely of Josiah Foster and Samuel Clement; Indian papers (1777-1819) and pamphlets (1778-1819) 1 box, in which the former section includes papers pertaining to Indians and the sale of lands in New Jersey and Charles Mooles' Vandeu[?] (sale) book, and the latter section contains pamphlets pertaining to legislative bills; New Jersey Land Society (ca. 1788), which was involved in the Burr Conspiracy; abolition societies; friends meetings; New Jersey elections; \"New Jersey State Gazette;\" and religious tracts. Miscellaneous legal papers (1762-1873), which includes judgments, bonds, agreements, papers on the Albertson vs. Norcross Case, involving Josiah Albertson (ca. 1753-1833) and Isiah Norcross, and other legal documents concerning Gloucester and Burlington counties; cash book, (1781-83) and dockets, (1790-94, 1798-1801) of Josiah Foster; and, land surveys (1687-1801), which includes an undated volume entitled \"Penn's Survey in New Jersey\" copies from the Surveyor General's books at Burlington, (1712-84), and road maps in and around Burlington County (ca. 1802-1846), which show detailed maps of surrounding tracts, and Atsion Company (of Josiah Foster) papers, (1764-99). </abstract>\n                  <name>\n                     <namePart>Historical Society of Pennsylvania</namePart>\n                     <role>\n                        <roleTerm valueURI=\"http://id.loc.gov/vocabulary/relators/rps\">Repository</roleTerm>\n                     </role>\n                  </name>\n               </mods>\n            </objectXMLWrap>",
                "title": "Foster-Clement Collection, ca. 1676-1875.",
                "abstract": "Papers relating to early settlements in and largely to the Clement and Foster families of Burlington and Gloucester Counties, New Jersey: deeds (1677-1875); wills (1681-1867); family papers (1761-1819), which includes papers and receipts of Josiah Foster and William Foster, draft of William Foster's land, will of William Foster, will of Josiah Foster, Clement and Foster genealogies, Long Book (account book) of Benjamin Robinson, (1784) and surveying notebooks of Josiah Foster (1778-79, 1794-95), Account of Sleepcreek Mill book, of which Josiah Foster was director, (1785-86), and correspondence largely of Josiah Foster and Samuel Clement; Indian papers (1777-1819) and pamphlets (1778-1819) 1 box, in which the former section includes papers pertaining to Indians and the sale of lands in New Jersey and Charles Mooles' Vandeu[?] (sale) book, and the latter section contains pamphlets pertaining to legislative bills; New Jersey Land Society (ca. 1788), which was involved in the Burr Conspiracy; abolition societies; friends meetings; New Jersey elections; \"New Jersey State Gazette;\" and religious tracts. Miscellaneous legal papers (1762-1873), which includes judgments, bonds, agreements, papers on the Albertson vs. Norcross Case, involving Josiah Albertson (ca. 1753-1833) and Isiah Norcross, and other legal documents concerning Gloucester and Burlington counties; cash book, (1781-83) and dockets, (1790-94, 1798-1801) of Josiah Foster; and, land surveys (1687-1801), which includes an undated volume entitled \"Penn's Survey in New Jersey\" copies from the Surveyor General's books at Burlington, (1712-84), and road maps in and around Burlington County (ca. 1802-1846), which show detailed maps of surrounding tracts, and Atsion Company (of Josiah Foster) papers, (1764-99). ",
                "extent": "4 linear ft.",
                "displayEntry": "Foster-Clement Collection, ca. 1676-1875.",
